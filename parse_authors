# Split into seperate lines:
split = reflist.split('\n')

# Match pattern to each line:
matched = list()
for string in split:
    if string:
        matched.extend([m for m in r.finditer(string)])

# Convert result matches to dictionary:
authors = list()
for g in matched:
    author = dict()
    for key, value in g.groupdict().items():
        author[key] = value.strip()
    if not author in authors:
        authors.append(author)

# Sort authors by last name:
authors = sorted(authors, key=lambda x: x['last_name'])

# Print each author dictionary:
for a in authors:
    print(a)
