# Get a list of unique authors from an APA-styled reference list using regular expressions.

import re

# Regular expression pattern matching APA referenced author name:
r = re.compile('(?:(?<=^)|(?<=, )(?:& )?)(?P<last_name>[A-Z-]\w+), (?P<initials>([A-Z]\. ?)+),?')

